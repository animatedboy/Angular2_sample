{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "build",
    "noEmitOnError": true,
    "noEmit":true,
    "listEmittedFiles":true,
    "allowJs":true
  }
}
